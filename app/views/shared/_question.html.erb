

<div id="question_card" class="card h-50">
  <div class="container">
    <div class="card-body mt-2">
      <h4 class="card-title">질문 리스트</h4>
      <h6 class="card-subtitle mb-2 text-muted">Question List</h6>
      <% @posts.each do |post| %>
      <p class="card-text"><a href="/posts/<%= post.id %>"><%= post.title %></a></p>
      <% end %>
    </div>
  </div>
</div>